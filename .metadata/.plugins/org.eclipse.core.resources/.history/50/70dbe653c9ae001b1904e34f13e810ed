

object Objects extends App{

  // SCALA DOES NOT HAVE CLASS-LEVEL FUNCTIONALITY ("static")
  
  object Person {
    val N_EYES = 2
    def canFly: Boolean = false
  }
  
  class Person {
    // seperate instance level functionality
  }
  
  val mary = Person
  val john = Person
  
  println(mary == john)
  
  
  
}